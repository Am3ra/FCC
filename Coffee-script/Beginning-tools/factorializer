factorialize = (num) ->
	i = num - 1
	if num is 0
		num = 1
	while i > 0
		num *= i
		i--
	num

factorialize 5


// equivalent js

var factorialize;

factorialize = function(num) {
  var i;
  i = num - 1;
  if (num === 0) {
    num = 1;
  }
  while (i > 0) {
    num *= i;
    i--;
  }
  return num;
};

factorialize(5);
