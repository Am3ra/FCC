function confirmEnding (str, target) {
var same=true;
   for(i=1; i<=target.length; i++){
     if (target[target.length-i]===str[str.length - i]){
        same = true;
       console.log(same);
     } else {
       same = false;
     }
   }
   return same;
}

console.log(confirmEnding("Connor", "n"));
